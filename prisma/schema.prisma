generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Registration {
  id          Int      @id @default(autoincrement())
  fullName    String
  email       String   @unique
  phone       String?  
  university  String?  
  createdAt   DateTime @default(now())
}
